# This file is sourced after profile by interactive shells, but prior
# to login.

. ~/.shell/interactive

ssource ~/.zprezto/runcoms/zshrc

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/jboone/anaconda3/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/jboone/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/jboone/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/jboone/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

CONDA_VERSION=$(conda info --json | jq '.conda_version')

if [[ "${CONDA_VERSION:-NONE}" != "NONE" ]]; then
	case "${OSTYPE}" in
		darwin*)
			conda activate macosPrime
		;;
		*)
			conda activate base
		;;
	esac
fi
